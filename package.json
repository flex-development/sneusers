{
  "name": "@flex-development/sneusers",
  "version": "1.0.0-dev.0",
  "description": "Demo users system built with NestJS, SQL, and TypeScript.",
  "keywords": [
    "docker",
    "mysql",
    "nestjs",
    "sql",
    "sqlite",
    "typescript"
  ],
  "license": "BSD-3-Clause",
  "bugs": "https://github.com/flex-development/sneusers/issues",
  "homepage": "https://github.com/flex-development/sneusers",
  "repository": "git@github.com:flex-development/sneusers.git",
  "author": {
    "name": "Lexus Drumgold",
    "url": "https://github.com/unicornware"
  },
  "files": [
    "CHANGELOG.md",
    "Dockerfile",
    "dist",
    "docker-compose.yml",
    "docs"
  ],
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "build:dbdocs": "dbdocs build ./docs/DATABASE.dbml",
    "build:image": "docker build --build-arg DOPPLER_CONFIG=$DOPPLER_CONFIG --build-arg DOPPLER_PROJECT=$DOPPLER_PROJECT --build-arg DOPPLER_TOKEN=$DOPPLER_TOKEN --build-arg GH_PAT=$GH_PAT --build-arg NODE_ENV=$NODE_ENV --build-arg NPM_TOKEN=$NPM_TOKEN --build-arg PORT=$PORT --tag ghcr.io/flex-development/sneusers .",
    "build:image:dev": "doppler run -- yarn build:image",
    "build:image:prod": "doppler run -c=production -- yarn build:image",
    "check:dedupe": "yarn dedupe --check",
    "check:format": "prettier --check .",
    "check:install": "yarn dlx @yarnpkg/doctor .",
    "check:style": "eslint . --ext cjs,js,md,mjs,ts",
    "check:types": "tsc -p tsconfig.json --skipLibCheck",
    "check:upgrades": "yarn upgrade-interactive",
    "clean": "concurrently \"yarn:clean:*\"",
    "clean:build": "trash ./{dist,*.tgz}",
    "clean:modules": "trash .yarn/{cache,*.tgz} node_modules",
    "docker:prune": "docker system prune -f",
    "fix:format": "prettier --write ./",
    "fix:style": "yarn check:style --fix --cache",
    "postinstall": "is-ci || chmod +x .husky/*; husky install",
    "prepack": "doppler run -c=production -- yarn build",
    "release": "doppler run -c=production -- ts-node ./tools/cli/release",
    "run:app": "nest start",
    "run:dev": "NODE_ENV=development yarn run:app -wd",
    "run:prod": "NODE_ENV=production yarn run:app -w",
    "start:dev": "doppler run -- docker compose up app.dev",
    "start:prod": "doppler run -c=production -- docker compose up app",
    "stop:dev": "doppler run -- docker compose down app.dev",
    "stop:prod": "doppler run -c=production -- docker compose down app",
    "test": "bash ./tools/scripts/mocha.sh",
    "test:coverage": "yarn test; serve ./coverage/lcov-report"
  },
  "dependencies": {
    "@flex-development/exceptions": "7.0.1",
    "@flex-development/tutils": "4.4.0",
    "@nestjs/common": "8.2.0",
    "@nestjs/config": "1.1.0",
    "@nestjs/core": "8.2.0",
    "@nestjs/platform-express": "8.2.0",
    "@nestjs/sequelize": "8.0.0",
    "@nestjs/swagger": "5.1.4",
    "class-transformer": "0.4.0",
    "class-transformer-validator": "0.9.1",
    "class-validator": "0.13.1",
    "debug": "4.3.3",
    "express": "4.17.1",
    "mysql2": "2.3.3",
    "read-pkg": "5.2.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.4.0",
    "sequelize": "6.12.0-beta.3",
    "sequelize-typescript": "2.1.1",
    "sqlite3": "5.0.2",
    "volleyball": "1.5.1"
  },
